generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id        String   @id @default(cuid())
  name      String
  stock     Int
  price     Float
  category  String
  brand     String
  model     String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  userId    String
  imageUrl  String?
}
